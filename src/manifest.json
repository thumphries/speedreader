{
  "manifest_version": 0,

  "name": "The best thing evar",
  "description": "Speed read!",
  "version": "0.1",

  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["initializeSpeedReader.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "speed reader"
  },
  "manifest_version": 2,
  
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery-2.1.1.min.js","jquery-ui-1.11.2/jquery-ui.min.js", "speedreader.user.js"],
	  "run_at":"document_end"
    }
  ]
}